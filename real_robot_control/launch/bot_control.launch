<launch>
    <arg name="model" default="$(find real_robot_description)/urdf/real_robot.gazebo.xacro"/>

  <param name="robot_description" command="$(find xacro)/xacro --inorder $(arg model)" /> 

    <rosparam command="load"
              file="$(find real_robot_control)/config/control.yaml" />


    <node name="controller_spawner" pkg="controller_manager" type="spawner"
  args="real_robot_velocity_controller real_robot_joint_publisher"/>

  
   


</launch>
